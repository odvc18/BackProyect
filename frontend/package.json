{
  "name": "backproyect-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend para el sistema BackProyect - Gestión de concursos académicos",
  "scripts": {
    "dev": "vite",
    "dev:simple": "vite --config vite.config.simple.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:simple": "tsc --noEmit --project tsconfig.simple.json",
    "clean-install": "rm -rf node_modules package-lock.json && npm install",
    "clean": "rm -rf node_modules/.vite dist"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.18.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/node": "^20.10.4",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "material-ui",
    "redux",
    "microservices",
    "academic-contests"
  ],
  "author": "Equipo BackProyect",
  "license": "MIT"
}
